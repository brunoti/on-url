  },
  "author": "Bruno Oliveira <bruno.oliveirati1@gmail.com> (https://github.com/brunoti)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-env": "^2.0.9",
    "browserify": "^13.1.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.7.1",
    "eslintify": "^3.0.0",
    "jsdom": "^9.6.0",
    "nyc": "^8.3.1",
    "simulant": "^0.2.2",
    "sinon": "^1.17.6",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*"
    ]
  },
